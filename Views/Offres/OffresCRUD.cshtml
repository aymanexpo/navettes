@model IEnumerable<navettes.Models.Offre>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NmbAbone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stiuation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prix)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Navette.fromCity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Navette.toCity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Navette.timeGo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Navette.timeWent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.NmbAbone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Stiuation)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.prix)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Navette.fromCity)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Navette.toCity)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Navette.timeGo)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Navette.timeWent)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
        @Html.ActionLink("Details", "Details", new { id = item.id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.id })
    </td>
</tr>
}

</table>
